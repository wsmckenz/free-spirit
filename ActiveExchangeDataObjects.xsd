<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2016 sp1 (x64) (http://www.altova.com) by Chris Verret (EnergyIQ Exchange) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:aexDo="http://www.energyiqexchange.com/ActiveExchangeDataObjects" targetNamespace="http://www.energyiqexchange.com/ActiveExchangeDataObjects" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:element name="AexDataset">
		<xs:annotation>
			<xs:documentation>Container for objects in support of ActiveExchange data transfer/exchange</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="WellOrigins" type="aexDo:WellOriginType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WellOrigin" type="aexDo:WellOriginType">
		<xs:annotation>
			<xs:documentation>Location on the surface of the earth where a drill bit is planned to penetrate or does penetrate the earth to establish or rework a well</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Wellbore" type="aexDo:WellboreType">
		<xs:annotation>
			<xs:documentation>Path of drilled footage from the Well Origin (top/start) to a terminating point (bottom/end)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Trajectory" type="aexDo:TrajectoryType">
		<xs:annotation>
			<xs:documentation>The trajectory (departure from vertical and direction of departure) of the wellbore; aka directional survey</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TrajectoryStation" type="aexDo:TrajectoryStationType">
		<xs:annotation>
			<xs:documentation>An individual trajectory station</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Marker" type="aexDo:MarkerType">
		<xs:annotation>
			<xs:documentation>Positional data on a formation, marker, contact or horizon (aka Top, aka Pick) within a well</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SpatialLocation" type="aexDo:SpatialLocationType">
		<xs:annotation>
			<xs:documentation>Represents the spatial location or coordinate of an object</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LegalLocation" type="aexDo:LegalLocationType">
		<xs:annotation>
			<xs:documentation>Container for a legal description associated with a Well or Wellbore</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="OffshoreLegalLocation" type="aexDo:OffshoreLegalLocationType">
		<xs:annotation>
			<xs:documentation>Offshore legal description associated with a Well or Wellbore</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TexasLegalLocation" type="aexDo:TexasLegalLocationType">
		<xs:annotation>
			<xs:documentation>Texas legal description associated with a Well or Wellbore</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CongressionalLegalLocation" type="aexDo:CongressionalLegalLocationType">
		<xs:annotation>
			<xs:documentation>Congressional legal description associated with a Well or Wellbore</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Extension" type="aexDo:ExtensionType">
		<xs:annotation>
			<xs:documentation>Used to extend any data object with implementation-specific attributes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Metadata" type="aexDo:MetadataType">
		<xs:annotation>
			<xs:documentation>Contains metadata for a data object (e.g., last updated by, last updated date, created by, created date)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Reference" type="aexDo:ReferenceType">
		<xs:annotation>
			<xs:documentation>Reference elements that contain Code, ShortName, LongName, Abbreviation, etc, such as Province/State, County, Depth Datum</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WellboreCompletion" type="aexDo:WellboreCompletionType">
		<xs:annotation>
			<xs:documentation>A set of one or more wellbore contact intervals that function as a single unit to produce or inject fluids</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Completion" type="aexDo:CompletionType">
		<xs:annotation>
			<xs:documentation>a completion interval associated with a Wellbore Completion</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="WellOriginType">
		<xs:annotation>
			<xs:documentation>Location on the surface of the earth where a drill bit is planned to penetrate or does penetrate the earth to establish or rework a well</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Uid" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique identifier of this data object (system-generated)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Source" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Source (individual, company or government agency) of this data object</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GovernmentId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifier assigned to the Well by a regulatory agency (e.g., API, UWI)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name assigned to the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number assigned to the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LeaseName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name assigned to the lease by the regulatory agency with jurisdiction over the minerals</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LeaseNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number assigned to the lease by the regulatory agency with jurisdiction over the minerals</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SurfaceSpatialLocation" type="aexDo:SpatialLocationType" minOccurs="0"/>
			<xs:element name="SurfaceLegalLocation" type="aexDo:LegalLocationType" minOccurs="0"/>
			<xs:element name="OriginalOperator" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The original operator of the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrentOperator" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The current operator of the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Country" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The country in which the Well is physically located (e.g., US)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProvinceState" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The province or state in which the Well is physically located (e.g., AB, TX)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="County" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The county in which the well is physically located (e.g., Webb); USA only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Region" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The geographic region in which the Well is physically located</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Field" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The field the Well is assigned to for production purposes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Basin" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The basin or geologic region (with similar features) in which the Well is physically located</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="District" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The district in which the Well is physically located</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Environment" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of environment that in which the well is physically located (e.g., LAND, MARINE)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InitialClassification" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Initial classification of the Well  (e.g., NEW_FIELD_WILDCAT, OUTPOST)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrentStatus" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Current status of the Well (e.g., PROPOSED, LICENSED, SPUDDED, DRILLING, COMPLETING, PRODUCING)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FluidType" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of flUid associated with this Well (e.g., GAS, OIL, CONDENSATE, WATER)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElevRefDatum" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The point from which all elevations are measured (e.g., MSL, LAT)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElevRefDatumEpsgCode" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The EPSC Code associated with the point from which all elevations are measured (e.g., 5714 for MSL)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroundElev" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elevation of the ground at the Well site</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WaterDepthDatum" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The point from which water depth is referenced (e.g., MSL)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WaterDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Depth of the water at the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LicenseDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date the Well was licensed or permitted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OnProductionDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when this Well first begins producing flUids (comes on production)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpudDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date the drilling operations commenced on the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Confidential" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies whether this Well is tight (confidential) or not</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConfidentialDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date confidential information may be released to the general public</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remarks associated with the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepthUom" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Units of measure associated with depth values (e.g., FT, M)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElevUom" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Units of measure associated with elevation values</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboresRef" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains an optional URI to support lazy-loading of Wellbores; URI could be used to subsequently return the set of associated Wellbores.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Wellbores" type="aexDo:WellboreType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of associated Wellbore data objects.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="aexDo:Metadata" minOccurs="0"/>
			<xs:element ref="aexDo:Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WellboreType">
		<xs:annotation>
			<xs:documentation>Path of drilled footage from the Well Origin (top/start) to a terminating point (bottom/end)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Uid" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique identifier of this data object (system-generated)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UidParent" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique identifier of the associated parent object
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Source" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Source (individual, company or government agency) of this data object</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GovernmentId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifier assigned to the Wellbore by a regulatory agency (e.g., API, UWI)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboreName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of this Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboreNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of this Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BHSpatialLocation" type="aexDo:SpatialLocationType" minOccurs="0"/>
			<xs:element name="BHLegalLocation" type="aexDo:LegalLocationType" minOccurs="0"/>
			<xs:element name="CurrentClassification" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Current classification of the Wellbore (e.g., NEW_FIELD_WILDCAT, OUTPOST)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrentStatus" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Status of this Wellbore (e.g., DRILLING, COMPLETING, PRODUCING)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FluidType" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of flUid associated with this Wellbore (e.g., GAS, OIL, CONDENSATE, WATER)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProfileType" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shape or profile of this Wellbore (e.g., VERTICAL, HORIZONTAL, DIRECTIONAL)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboreType" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of Wellbore (e.g., SIDETRACK, BYPASS, PILOT)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepthDatum" type="aexDo:ReferenceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The point from which depths are measured (e.g., KB, RT, DF, CF)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepthDatumElev" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elevation associated with the DepthDatum value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CasingFlangeElev" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elevation of the casing flange</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DerrickFloorElev" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elevation of the derrick floor</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KellyBushingElev" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elevation of the kelly bushing (generally from sea level)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RotaryTableElev" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elevation of the rotary table</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DrillerTotalDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The maximum depth of the Wellbore as reported by the driller or operator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KickoffPointDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Measured depth of the kickoff point of the Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlugbackDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The depth of the point that the Wellbore was plugged back when setting casing or completing the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The actual measurement along the Well path from reference datum to the deepest point in the Well</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrueVerticalDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The true vertical depth measurement from reference datum to the deepest point, measured on a straight vertical line</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WhipstockDepth" type="xs:float" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The depth of the whipstock (used for sidetracking or directional drilling)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FormationAtTD" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the Formation at Total Depth</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AbandonmentDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date the Wellbore was plugged and abandoned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CompletionDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on the completion report indicating the Well is ready to produce, inject or abandon</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinalDrillDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date the drilling operations were completed on this Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OnProductionDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when this Wellbore first begins producing flUids (comes on production)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RigReleaseDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when the drilling ris released from operations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpudDate" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date the drilling operations commenced on the Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RigId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifier associated with the drilling rig</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remarks associated with the Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepthUom" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Units of measure associated with depth values (e.g., FT, M)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElevUom" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Units of measure associated with elevation values</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboresRef" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains an optional URI to support lazy-loading of Wellbores; URI could be used to subsequently return the set of associated Wellbores.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Wellbores" type="aexDo:WellboreType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of associated Wellbores data objects (children of this Wellbore)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboreCompletionsRef" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains an optional URI to support lazy-loading or subsequent loading of Wellbore Completions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WellboreCompletions" type="aexDo:WellboreCompletionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of associated Wellbore Completion data objects (children of this Wellbore)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrajectorysRef" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains an optional URI to support lazy-loading of Trajectory data objects; URI could be used to subsequently return the set of associated Trajectory data objects.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Trajectorys" type="aexDo:TrajectoryType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
