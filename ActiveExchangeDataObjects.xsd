			</xs:element>
			<xs:element name="Remark" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remark associated with the Wellbore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepthUom" type="xs:string" minOccurs="0">
